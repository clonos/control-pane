{
  "name": "babel-plugin-import-redirect",
  "version": "1.1.1",
  "description": "Import, export, require path redirect plugin for Babel",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "test": "jest --config='./__tests__/config.json'",
    "test:coverage": "npm run test -- --coverage --silent",
    "lint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore --cache",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "Velenir <velenir@gmail.com>",
  "license": "MIT",
  "keywords": [
    "babel",
    "babel-plugin",
    "import",
    "redirect",
    "require",
    "export",
    "path",
    "rewrite",
    "resolve",
    "replace",
    "rename",
    "mock",
    "module"
  ],
  "dependencies": {
    "babylon": "^6.17.4",
    "resolve": "^1.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "21.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "4.16.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jest": "21.7.0",
    "jest": "^19.0.2",
    "semantic-release": "^12.2.2"
  },
  "engines": {
    "node": ">=4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velenir/babel-plugin-import-redirect.git"
  }
}
